{
	"name": "kanban-view",
	"version": "1.1.1",
	"description": "Une vue Kanban pour les Bases d'Obsidian",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "changeset version && node .changeset/version-bump.mjs && git add manifest.json versions.json CHANGELOG.md",
		"changeset": "changeset",
		"changeset:version": "changeset version",
		"changeset:status": "changeset status",
		"release": "npm run build && changeset publish",
		"release:create": "npm run build && ./scripts/create-release.sh"
	},
	"keywords": [],
	"author": "",
	"license": "MIT",
	"devDependencies": {
		"@changesets/cli": "^2.29.7",
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"obsidian": "latest",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	}
}
